{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-imports": "warn",
        "global-require": "warn",
        "import/no-dynamic-require": "off"
    }
}
